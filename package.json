{
  "name": "react-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx dev-server.ts",
    "infra:up": "docker-compose up -d",
    "infra:down": "docker-compose down",
    "db:migrate": "npx drizzle-kit push",
    "sam:build": "sam build",
    "sam:local": "sam local start-api --env-vars locals.json --docker-network betterhr-net",
    "dev:sam": "npm run infra:up && npm run db:migrate && npm run sam:build && npm run sam:local",
    "start": "tsx dev-server.ts",
    "build": "tsc",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.31.0",
    "@aws-lambda-powertools/metrics": "^2.31.0",
    "@aws-lambda-powertools/tracer": "^2.31.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.996.0",
    "@aws-sdk/client-s3": "^3.996.0",
    "@aws-sdk/s3-request-presigner": "^3.996.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "express": "^4.21.2",
    "pg": "^8.18.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.160",
    "@types/express": "^4.17.21",
    "@types/node": "^22.19.11",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.31.9",
    "esbuild": "^0.27.3",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2"
  }
}
